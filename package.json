{
  "name": "zulaica-dot-info",
  "author": "David Zulaica",
  "version": "2.0.0",
  "engines": {
    "node": ">=16.16.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build:styles": "postcss source/styles/main.css --output build/stylesheet.min.css",
    "dev": "browser-sync source --watch --no-ui --port 50001",
    "lint": "npm run lint:js || true && npm run lint:css",
    "lint:css": "stylelint \"source/**/*.css\" --custom-formatter ./lib/formatters/stylelint.js",
    "lint:js": "eslint \"source/**/*.js\" \"lib/**/*.*js\" --format ./lib/formatters/eslint.js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "browser-sync": "^2.27.10",
    "cssnano": "^5.1.13",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.16",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0"
  },
  "volta": {
    "node": "16.16.0",
    "npm": "8.13.2"
  }
}
